{
  "port": {%= port %},
  "logging" : {
    "file" : "tmp/development.log",
    "console" : true,
    "level" : "debug"
  },
  "monitoring" : {
    "subscribers": {
      "console" : ["ops", "request", "log", "error"],
      "tmp/development.log" : ["ops", "request", "log", "error"]
    }
  },
  "aws": {
    "accessKeyId" : "CHANGEME",
    "secretAccessKey" : "CHANGEME",
    "region": "us-east-1"
  },
  "dynamodb" : {
    "endpoint" : "http://localhost:8000"
  },
  "api": {
    "url": "http://api.localhost.com:9000"
  },
  "timeout" : 3500,
  "metrics" : false
}
