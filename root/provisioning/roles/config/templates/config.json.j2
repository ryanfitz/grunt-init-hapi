{

  "port": {{ http_port }},
  "logging" : {
    "file" : "{{ log_file }}",
    "console" : true,
    "level" : "debug"
  },
  "monitoring" : {
    "subscribers": {
      "console" : ["ops", "request", "log", "error"],
      "{{ log_file }}" : ["ops", "request", "log", "error"]
    }
  },
  "aws": {
    "accessKeyId" : "{{ stack.stack_outputs.AccessKeyId }}",
    "secretAccessKey" : "{{ stack.stack_outputs.SecretAccessKey }}",
    "region": "us-east-1"
  },
  "dynamodb" : {
    "tables" : {
    }
  },
  "api": {
    "url": "{{ api.url }}"
  },
  "timeout" : 3500,
  "metrics" : {{ metrics }}
}
